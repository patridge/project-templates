<Addin id          = "iOSStarterTemplate"
       namespace   = "PatridgeDev.iOSStarterTemplate"
       name        = "Custom Xamarin.iOS Project"
       author      = "Adam Patridge"
       copyright   = "MIT"
       url         = "http://www.patridgedev.com/"
       description = "Just a custom starter project for Xamarin.iOS."
       category    = "Templates"
       version     = "0.1.0">

  <Runtime>
    <Import file = "CustomXamariniOSProject.xpt.xml"/>
    <Import file = "LICENSE" />
    <Import file = "README.md" />
  </Runtime>

  <Dependencies>
    <Addin id = "Ide" version="4.2.3"/>
    <Addin id = "CSharpBinding" version = "4.2.3" />
  </Dependencies>

  <Extension path = "/MonoDevelop/Ide/ProjectTemplates">
    <ProjectTemplate id   = "iOSStarterTemplate"
                     file = "CustomXamariniOSProject.xpt.xml"/>
  </Extension>

  <!--<Extension path = "/MonoDevelop/Ide/FileTemplates">
    <FileTemplate id   = "OpenOfficeSpreadsheetDocHelper"
                  file = "OpenOfficeSpreadsheetDocHelper.xft.xml"/>
  </Extension>-->
</Addin>
